{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "packageRules": [
    {
      "matchDatasources": ["docker"],
      "matchPackagePatterns": ["cloudnative-pg/postgresql"],
      "allowedVersions": "<17"
    },
    {
      "matchDatasources": ["docker"],
      "matchPackagePatterns": ["quay.io/ceph/ceph"],
      "allowedVersions": "<v19"
    }
  ]
}
