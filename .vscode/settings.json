    "kubernetes/**/*.sops.toml": "plaintext",
    "terraform/**/*.tf": "terraform"
  },
  "files.exclude": {
    ".direnv/": true,
    "result": true
  },
  "material-icon-theme.folders.associations": {
    ".github": "Github",
    "workflows": "Robot",
    ".taskfiles": "utils",
    "hack": "Tools",
    "kubernetes": "Cluster",
    "kubernetes/main": "cluster",
    "kubernetes/staging": "cluster",
    "cert-manager": "guard",
    "dbms": "database",
    "default": "cluster",
    "dev": "less",
    "flux-system": "kubernetes",
    "kube-system": "kubernetes",
    "media": "video",
    "monitoring": "event",
    "networking": "connection",
    "outline": "docs",
    "security": "keys",
    "self-hosted": "app",
    "setup": "import",
    "config": "environment"
  },
  "prettier.configPath": ".ci/prettier/.prettierrc.yaml",
  "prettier.ignorePath": ".ci/prettier/.prettierignore",
  "yaml.schemas": {
    "Kubernetes": "kubernetes/*.yaml",
    "https://json.schemastore.org/kustomization.json": "file:///Users/scotte/Development/k8s-homelab/kubernetes/apps/tekton/tasks/kustomization.yaml"
  },
  "yaml.customTags": ["!env_var"],
  "explorer.autoReveal": false,
